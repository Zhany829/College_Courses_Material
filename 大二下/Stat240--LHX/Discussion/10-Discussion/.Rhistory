library(tidyverse)
library(lubridate)
source("../../scripts/viridis.R")
source("../../scripts/ggprob.R")
## Read and transform the Madison weather data
mw <- read_csv("../../data/madison-weather-2020-clean-2.csv")
mw_july <- mw %>%
filter(month=="Jul") %>%
filter(year < 1899 | year > 1990) %>%
group_by(year) %>%
summarize(tavg = mean(tavg)) %>%
mutate(era = case_when(
year < 1900 ~ "early",
year > 1989 ~ "recent"))
#view(mw_july)
q1 = mw_july%>%
group_by(era)%>%
summarize(mean = mean(tavg), sd = sd(tavg))
q1
mw_july%>%
group_by(era)%>%
ggplot(aes(x = tavg, color = era)) +
geom_density()
x1 <- mw_july %>%
filter(era=="early") %>%
pull(tavg)
t.test(x1)
x2 <- mw_july %>%
filter(era=="recent") %>%
pull(tavg)
t.test(x2)
#early era CI
# sd
s1 = q1%>%
filter(era=="early")%>%
pull(sd)
#xbar
xbar1 = q1%>%
filter(era=="early")%>%
pull(mean)
# t*
x1t = qt(.025, 29)
early_CI <- tibble(early_UCL = xbar1 + (x1t * (s1 / sqrt(30))),
early_LCL = xbar1 - (x1t * (s1 / sqrt(30)))
)
early_CI
#recent era CI
# sd
s2 = q1%>%
filter(era=="recent")%>%
pull(sd)
#xbar
xbar2 = q1%>%
filter(era=="recent")%>%
pull(mean)
recent_CI <- tibble(recent_UCL = xbar2 + (x1t * (s2 / sqrt(30))),
recent_LCL = xbar2 - (x1t * (s2 / sqrt(30)))
)
recent_CI
gt(df = 29)+
